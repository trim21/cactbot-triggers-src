{
  "name": "cactbot-triggers",
  "version": "1.0.0",
  "description": "Generate user config files for cactbot with TypeScript",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "private": true,
  "scripts": {
    "lint": "eslint --ext ts,js,vue ./triggers/",
    "format": "prettier -w ./",
    "triggers": "yarn workspace triggers",
    "prebuild": "rimraf ./dist/",
    "build": "webpack --config webpack.triggers.config.js --config webpack.ui.config.js",
    "start": "yarn build --watch",
    "start:ui": "webpack --config webpack.ui.config.js --watch",
    "update-submodules": "git submodule foreach git pull",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,cjs,mjs,json}": "prettier -w"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/types": "^7.19.4",
    "@typescript-eslint/eslint-plugin": ">=5.40.1",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.26.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "vue-eslint-parser": "^9.1.0",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.7.13",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@trim21/cactbot": "0.27.17",
    "plain-object-is-equal": "^2.0.0",
    "vue": "^3.2.41",
    "vuedraggable": "^4.1.0"
  }
}
