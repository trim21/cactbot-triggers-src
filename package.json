{
  "name": "cactbot-triggers",
  "version": "1.0.0",
  "description": "Generate user config files for cactbot with TypeScript",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "private": true,
  "scripts": {
    "lint": "eslint --ext ts,js,vue,mjs,cjs .",
    "format": "prettier -w ./",
    "triggers": "yarn workspace triggers",
    "prebuild": "rimraf ./dist/",
    "build": "webpack --config webpack.triggers.config.mjs --config webpack.ui.config.mjs",
    "start": "yarn build --watch",
    "start:ui": "webpack --config webpack.ui.config.mjs --watch",
    "update-submodules": "git submodule foreach git pull",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,cjs,mjs}": [
      "eslint --fix"
    ],
    "*.{ts,js,cjs,mjs,json,yml,vue}": [
      "prettier -w"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/types": "^7.21.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.42.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "vue-eslint-parser": "^9.1.1",
    "vue-loader": "^17.1.0",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "cactbot": "0.28.18",
    "vue": "^3.2.47",
    "vuedraggable": "^4.1.0"
  },
  "packageManager": "yarn@3.6.0"
}
