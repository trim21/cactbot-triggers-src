{
  "name": "triggers",
  "version": "0.0.1",
  "description": "Generate user config files for cactbot with TypeScript",
  "repository": {
    "type": "git+https",
    "url": "https://github.com/MaikoTan/cactbot-user-template.git"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "format": "prettier -w ./",
    "prebuild": "rimraf ./dist/",
    "build": "webpack --config webpack.triggers.config.js --config webpack.ui.config.js",
    "start:ui": "webpack --config webpack.ui.config.js --watch",
    "update-submodules": "git submodule foreach git pull"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "esbuild": "^0.15.10",
    "esbuild-loader": "^2.20.0",
    "html-webpack-plugin": "^5.5.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.7.10",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "overlay-plugin": "^0.0.0",
    "cactbot": "^0.27.12",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.40",
    "vuedraggable": "^4.1.0"
  }
}
